%h2 Sign in
= render partial: 'flash'
.sign-in
  = f_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
    = f.text_field :name
    = f.password_field :password
    - if devise_mapping.rememberable?
      = f.check_box :remember_me
    = f.submit "Sign in"
= render "devise/shared/links"
